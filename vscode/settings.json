{
    "editor.fontSize": 14,
    "workbench.colorTheme": "Monokai",
    "phpcs.standard": "PSR2",
    "php.validate.executablePath": "/usr/bin/php",
    "workbench.activityBar.visible": true,
    "explorer.openEditors.visible": 0,
    "editor.minimap.enabled": false,
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
    "php-cs-fixer.rules": {
        "@PSR2": true,
        "@Symfony": true,
        "@DoctrineAnnotation": true,
        "no_unused_imports": true,
        "ordered_class_elements": true,
        "ordered_imports": true,
        "phpdoc_add_missing_param_annotation": true,
        "phpdoc_no_empty_return": true,
        "array_syntax": {
            "syntax": "short"
        }
    },
    "phpcs.ignorePatterns": [
        "*Spec.php"
    ],
    "php-cs-fixer.exclude": [
        "*Spec.php"
    ],
    "git.enableCommitSigning": true,
    "files.insertFinalNewline": true,
    "plantuml.exportOutDir": "diagrams",
    "explorer.compactFolders": false,
    "[php]": {
        "editor.defaultFormatter": "junstyle.php-cs-fixer"
    },
    "php-cs-fixer.lastDownload": 1645971298824,
    "workbench.startupEditor": "none",
    "go.buildOnSave": "off",
    "go.coverOnSingleTest": true,
    "go.coverOnSingleTestFile": true,
    "go.inlayHints.constantValues": true,
    "go.testEnvFile": "",
    "go.testExplorer.packageDisplayMode": "nested",
    "go.testExplorer.showDynamicSubtestsInEditor": true,
    "go.buildTags": "all_tests",
}
